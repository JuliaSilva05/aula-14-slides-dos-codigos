#include <stdio.h>
#include <stdlib.h>
//a partir daqui os imports ser√£o ignorados
int main(void) {
  int *p_array = malloc(10*sizeof(int));
  if (p_array != NULL) {
    for (int i = 0; i < 10; i++) {
      *(p_array+i) = i;
      printf("%d ", p_array[i]);
    }
    free(p_array);
    p_array = NULL;
  }
   
  return 0;
}
