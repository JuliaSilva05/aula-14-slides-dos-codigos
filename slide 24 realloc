#include <stdio.h>
#include <stdlib.h>

int main(void) {
  int *p_array = malloc(10*sizeof(int));
  if (p_array != NULL) {
    for (int i = 0; i < 10; i++) {
      *(p_array+i) = i;
    }
    p_array = (int*) realloc(p_array, (20)*sizeof(int));
    if (p_array != NULL) {
      for (int i = 10; i < 20; i++) {
        *(p_array+1) = i;
      }
    }
    printf("%p ", p_array); //print adicionado por mim (julia)
    free(p_array);
    p_array = NULL;
  }

  return 0;
}
